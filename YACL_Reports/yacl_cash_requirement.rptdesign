            <xml-property name="queryText"><![CDATA[-- Cashbook Part
SELECT 
	aj.name ->> 'en_US' AS journal,
	ac.currency_id,
	pt.complete_name AS partner,
	COUNT(pt.id) AS count,
	COALESCE(SUM(total_amount), 0) AS amount,
	'Cashbook' AS source
FROM account_cashbook ac
LEFT JOIN res_partner pt ON pt.id = ac.partner_id
LEFT JOIN account_journal aj ON aj.id = ac.journal_id
WHERE ac.state = 'draft' 
  AND ac.id IS NOT NULL
GROUP BY aj.name, ac.currency_id, pt.complete_name

UNION ALL

-- Vendor Bill Part
SELECT 
	aj.name ->> 'en_US' AS journal,
	am.currency_id,
	pt.complete_name AS partner,
	COUNT(pt.id) AS count,
	COALESCE(SUM(aml.credit), 0) AS amount,
	'Vendor Bill' AS source
FROM account_move am
LEFT JOIN account_move_line aml ON aml.move_id = am.id
LEFT JOIN account_account aa ON aa.id = aml.account_id
LEFT JOIN res_partner pt ON pt.id = am.partner_id
LEFT JOIN account_journal aj ON aj.id = am.journal_id
WHERE am.state = 'posted'
  AND am.payment_state = 'not_paid'
  AND am.move_type = 'in_invoice'
  AND aa.account_type = 'liability_payable'
GROUP BY aj.name, am.currency_id, pt.complete_name;
]]></xml-property>
